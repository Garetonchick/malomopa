{
    "http_server": {
        "host": "localhost",
        "port": 5252
    },
    "logger": {
        "level": "info",
        "encoding": "json",
        "output_paths": [
            "log"
        ]
    },
    "scylla": {
        "nodes": [
            "172.18.0.2",
            "172.18.0.3",
            "172.18.0.4"
        ],
        "port": 9042,
        "keyspace": "data",
        "consistency": "ONE",
        "num_retries": 3
    },
    "cache_service": {
        "data_sources": [
            {
                "name": "general_order_info",
                "endpoint": ""
            },
            {
                "name": "zone_info",
                "endpoint": "",
                "deps": ["general_order_info"],
                "cache": {
                    "name": "lru",
                    "ttl": "10m",
                    "max_size": 1000
                }
            },
            {
                "name": "executor_profile",
                "endpoint": ""
            },
            {
                "name": "assign_order_configs",
                "endpoint": "",
                "cache": {
                    "name": "lru",
                    "ttl": "1m",
                    "max_size": 1
                }
            },
            {
                "name": "toll_roads_info",
                "endpoint": "",
                "deps": ["zone_info"]
            }
        ]
    }
}
